"pages/edit.js","name":"EditNote","original":{"line":33,"column":15},"generated":{"line":1,"column":1796}}],"sources":{"pages/edit.js":"import React from \"react\";\r\nimport { useQuery, useMutation } from \"@apollo/client\";\r\nimport {GET_ME, GET_NOTE} from \"../gql/query\";\r\nimport NoteForm from \"../components/NoteForm\";\r\nimport {EDIT_NOTE} from \"../gql/mutation\";\r\n\r\nconst EditNote = props => {\r\n    const id = props.match.params.id;\r\n\r\n    const { data } = useQuery(GET_NOTE, { variables: { id } });\r\n\r\n    const { loading, error, data: userdata } = useQuery(GET_ME);\r\n\r\n    const [editNote] = useMutation(EDIT_NOTE, {\r\n        variables: {\r\n            id\r\n        },\r\n        onCompleted: () => {\r\n            props.history.push(`/note/${id}`);\r\n        }\r\n    });\r\n\r\n    if (loading) return 'Loading...';\r\n    if (error) return <p>Error! Note not found</p>;\r\n\r\n    if (userdata.me.id !== data.note.author.id) {\r\n        return <p>You do not have access to edit this note</p>;\r\n    }\r\n\r\n    return <NoteForm content={data.note.content} action={editNote} />;\r\n};\r\n\r\nexport default EditNote;\r\n"},"lineCount":null}},"error":null,"hash":"432739570022fc58180ddf3028d5445e","cacheData":{"env":{}}}